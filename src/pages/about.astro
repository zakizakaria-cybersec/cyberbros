<MainLayout>
  <Navigation />
  
  <!-- Hero Section -->
  <section class="relative bg-gradient-to-b from-gray-950 via-gray-900 to-black pt-32 pb-24 px-6 overflow-hidden">
    <!-- Decorative elements -->
    <div class="absolute inset-0 opacity-20">
      <div class="absolute top-20 left-10 w-72 h-72 bg-indigo-600 rounded-full filter blur-3xl"></div>
      <div class="absolute bottom-20 right-10 w-96 h-96 bg-purple-600 rounded-full filter blur-3xl"></div>
    </div>
    
    <div class="max-w-5xl mx-auto text-center relative z-10">
      <div class="inline-block mb-4 px-4 py-2 bg-indigo-600/20 border border-indigo-500/30 rounded-full">
        <span class="text-indigo-400 text-sm font-semibold">üõ°Ô∏è About CyberBros</span>
      </div>
      <h1 class="text-5xl md:text-6xl font-bold text-white mb-6 leading-tight">
        Empowering Digital <span class="text-transparent bg-clip-text bg-gradient-to-r from-indigo-400 to-purple-400">Safety</span>
